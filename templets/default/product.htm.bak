	$(".p-product_item_left_lists li div").on("click","a",function(e){
		if(e.target.className!="p-product_item_left_more"){
			if($(this).parent().siblings("ul").hasClass("onctive")){
				$(this).parent().siblings("ul").slideUp(500).removeClass('onctive');
			}else{
				$(this).parent().siblings("ul").slideDown(500).addClass('onctive');
			}
		}
	});
